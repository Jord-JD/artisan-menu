{
    "name": "jord-jd/artisan-menu",
    "description": "Use Artisan via an elegant console GUI",
    "license": "LGPL-3.0-only",
    "require": {
        "php": ">=7.0",
        "php-school/cli-menu": "^3.0",
        "divineomega/json-key-value-store": "^1.0",
        "ext-json": "*",
        "laravel/framework": "^5.1||^6.0"
    },
    "autoload": {
        "psr-4": {
            "DivineOmega\\ArtisanMenu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DivineOmega\\ArtisanMenu\\Providers\\ArtisanMenuServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "replace": {
        "divineomega/artisan-menu": "self.version"
    }
}
